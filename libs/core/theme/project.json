{
  "name": "core-theme",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/core/theme/src",
  "prefix": "lib",
  "projectType": "library",
  "tags": ["type:core", "scope:theme"],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/core/theme",
        "main": "libs/core/theme/src/index.ts",
        "tsConfig": "libs/core/theme/tsconfig.lib.json",
        "assets": []
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "reportsDirectory": "../../../coverage/libs/core/theme"
      }
    }
  }
}
